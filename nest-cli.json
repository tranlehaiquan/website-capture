{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/web/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/web/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/web",
  "projects": {
    "web": {
      "type": "application",
      "root": "apps/web",
      "entryFile": "main",
      "sourceRoot": "apps/web/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web/tsconfig.app.json"
      }
    },
    "web-capture": {
      "type": "application",
      "root": "apps/web-capture",
      "entryFile": "main",
      "sourceRoot": "apps/web-capture/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web-capture/tsconfig.app.json"
      }
    },
    "web-capture1": {
      "type": "application",
      "root": "apps/web-capture1",
      "entryFile": "main",
      "sourceRoot": "apps/web-capture1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/web-capture1/tsconfig.app.json"
      }
    },
    "my-library": {
      "type": "library",
      "root": "libs/my-library",
      "entryFile": "index",
      "sourceRoot": "libs/my-library/src",
      "compilerOptions": {
        "tsConfigPath": "libs/my-library/tsconfig.lib.json"
      }
    }
  }
}